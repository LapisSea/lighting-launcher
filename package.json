{
	"name": "lighting-launcher",
	"version": "0.0.5",
	"description": "Lighting Launcher",
	"main": "main.js",
	"scripts": {
		"start": "electron Main.js",
		"build": "electron-packager . lighting-launcher --out=build  --overwrite --asar --icon=./icon.ico --ignore=.bat --ignore=.editorconfig --ignore=.gitignore --ignore=icon.ico --ignore=package-lock.json --ignore=README.md"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LapisSea/lighting-launcher.git"
	},
	"author": "LapisSea",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/LapisSea/lighting-launcher/issues"
	},
	"homepage": "https://github.com/LapisSea/lighting-launcher#readme",
	"win": {
		"target": "squirrel",
		"icon": "./icon.ico"
	},
	"dependencies": {
		"electron-window-state": "^4.1.1",
		"shelljs": "^0.8.1"
	},
	"devDependencies": {
		"electron": "^1.8.4",
		"electron-packager": "^11.1.0"
	}
}
